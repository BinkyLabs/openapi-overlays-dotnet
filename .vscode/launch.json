{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "dotnet: build",
			"program": "${workspaceFolder}/src/tool/bin/Debug/net8.0/BinkyLabs.OpenApi.Overlays.Cli.dll",
			"args": [
                "apply",
                "D:/github/openai-openapi-pr/description.yaml",
                "-o",
                "D:/github/openai-openapi-pr/overlay/overlay.json",
                "-out",
                "D:/github/openai-openapi-pr/updated.yaml"
            ],
			"cwd": "${workspaceFolder}",
			"stopAtEntry": false,
			"console": "internalConsole"
		}
	]
}